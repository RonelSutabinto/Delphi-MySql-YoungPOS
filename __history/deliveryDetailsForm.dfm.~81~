  object jodetailsQ: TMyQuery
    Connection = DataModl.MyConnection1
    SQL.Strings = (
      'Select'
      ' f.*'
      'from'
      '('
      '  Select '
      '    jod.article,'
      
        '    ifnull(jod.quantity,0) - ifnull((Select sum(ifnull(d.quantit' +
        'y,0)) from deliverydtls d where d.idjodetails = jod.idjodetails)' +
        ',0) as quantity,'
      '    jod.amount,'
      '    jod.idjo,'
      '    jod.idjodetails'
      ''
      '  from jodetails jod where jod.idjo = :idjo'
      ') f where f.quantity>0')
    Left = 480
    Top = 112
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'idjo'
        Value = nil
      end>
    object jodetailsQarticle: TStringField
