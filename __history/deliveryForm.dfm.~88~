  object insertdeliveryDetailsQ: TMyQuery
    Connection = DataModl.MyConnection1
    SQL.Strings = (
      'insert into deliverydtls'
      '( idjodetails,  '
      '  idjo,'
      '  date,'
      '  article,'
      '  quantity,'
      '  amount,  '
      '  jono,'
      '  deliveryno,'
      '  iddelivery'
      ')'
      '  '
      '(Select '
      '  idjodetails,'
      '  idjo,'
      '  :date,'
      '  article, '
      
        '  (jodetails.quantity - ifnull((Select sum(ifnull(d.quantity,0))' +
        ' from deliverydtls d where d.idjodetails = jodetails.idjodetails' +
        '),0) ) as quantity,'
      '  amount,'
      '  :jono,'
      '  :deliveryno,'
      '  :iddelivery '
      'from jodetails where idjo = :idjo)'
      '')
    Left = 296
    Top = 176
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'date'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'jono'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'deliveryno'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'iddelivery'
        Value = nil
